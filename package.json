{
  "name": "taxi-backend",
  "version": "1.0.0",
  "description": "Backend of Taxi Application in TypeScript",
  "main": "src/app.ts",
  "bin": "app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "test:u": "jest -c jest.config.unit.js",
    "test:i": "jest -c jest.config.integration.js",
    "run": "ts-node -- -P tsconfig.json ./src/app.ts",
    "coverage": "jest --coverage",
    "build:prod": "tsc; cp package.json dist/package.json; cp docker.env dist/.env; cd dist; pkg . --targets node12-linux --output server-linux;",
    "build:docker": "tsc; cp package.json dist/package.json; cp docker.env dist/.env; cd dist; pkg . --targets node12-linux;",
    "watch": "tsnd --respawn ./src/app.ts --inspect",
    "coverage:u": "jest --coverage -c jest.config.unit.js",
    "coverage:i": "jest --coverage -c jest.config.integration.js",
    "model:generate": "npx typeorm-model-generator -d taxi -p 3306 -u root -x defaultpassword -e mysql -o ./src -a",
    "fixtures": "fixtures ./src/fixtures --config ./ormconfig.json --sync --require=ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Innomalist/taxi-backend.git"
  },
  "keywords": [
    "taxi",
    "backend"
  ],
  "author": "Me",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Innomalist/taxi-backend/issues"
  },
  "homepage": "https://github.com/Innomalist/taxi-backend#readme",
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/braintree": "^2.22.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/google__maps": "^0.5.13",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.9",
    "@types/redis": "^2.8.22",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "@types/socket.io": "^2.1.8",
    "@types/socketio-jwt": "0.0.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "node-mocks-http": "^1.8.1",
    "pkg": "^4.4.8",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typeorm-fixtures-cli": "^1.6.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@google/maps": "^1.1.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "braintree": "^2.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.12.1",
    "flutterwave-node": "^1.1.0",
    "json-2-csv": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "node-paystack": "^1.0.3",
    "paystack": "^2.0.1",
    "paystack-node": "^0.2.4",
    "razorpay": "^2.0.6",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^6.0.0",
    "request": "^2.88.2",
    "request-promise-any": "^1.0.8",
    "socket.io": "^2.3.0",
    "socketio-jwt": "^4.6.2",
    "stripe": "^8.56.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "xml-js": "^1.6.11"
  },
  "pkg": {
    "scripts": [
      "entities/*.js",
      "migration/*.js",
      "subscribers/*.js"
    ],
    "assets": []
  }
}
